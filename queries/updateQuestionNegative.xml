<query id="updateQuestionNegative" action="update">
    <tables>
        <table name="faq_questions" />
    </tables>
    <columns>
        <column name="negative" var="negative" />
		<column name="votes" var="votes" />
    </columns>
    <conditions>
        <condition operation="equal" column="question_srl" var="question_srl" notnull="notnull" />
    </conditions>
</query>
