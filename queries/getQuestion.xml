<query id="getDocument" action="select">
    <tables>
        <table name="faq_questions" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="question_srl" var="question_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
