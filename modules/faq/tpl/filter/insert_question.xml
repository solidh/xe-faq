<filter name="insert_question" module="faq" act="procFaqInsertQuestion">
  <form>
    <node target="question" required="true" minlength="1" maxlength="250" />
    <node target="answer" required="true" />
    <node target="category_srl" required="true" />
  </form>
  <response callback_func="completeQuestionInserted">
    <tag name="error" />
    <tag name="message" />
    <tag name="mid" />
    <tag name="document_srl" />
    <tag name="category_srl" />
  </response>
</filter>
